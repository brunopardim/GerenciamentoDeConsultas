                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
	C.id_consulta,
	P.nome					AS 'paciente',
	F.nome					AS 'medico',

	ODL.esferico			AS 'odl_esferico',
	ODL.cilindro			AS 'odl_cilindro',
	ODL.adicao				AS 'odl_adicao',
	ODL.eixo				AS 'odl_eixo',

	OEL.esferico			AS 'oel_esferico',
	OEL.cilindro			AS 'oel_cilindro',
	OEL.adicao				AS 'oel_adicao',
	OEL.eixo				AS 'oel_eixo',


	ODP.esferico			AS 'odp_esferico',
	ODP.cilindro			AS 'odp_cilindro',
	ODP.adicao				AS 'odp_adicao',
	ODP.eixo				AS 'odp_eixo',

	OEP.esferico			AS 'oep_esferico',
	OEP.cilindro			AS 'oep_cilindro',
	OEP.adicao				AS 'oep_adicao',
	OEP.eixo				AS 'oep_eixo'
FROM
	consulta C
INNER JOIN cliente P
	ON P.id_cliente = C.id_cliente
INNER JOIN funcionario F
	ON F.id_funcionario = C.id_medico
INNER JOIN receita R
	ON R.id_consulta = C.id_consulta
INNER JOIN diagnostico ODL
	ON ODL.id_diagnostico = R.olho_direito_longe
INNER JOIN diagnostico OEL
	ON OEL.id_diagnostico = R.olho_esquerdo_longe
INNER JOIN diagnostico ODP
	ON ODP.id_diagnostico = R.olho_direito_perto
INNER JOIN diagnostico OEP
	ON OEP.id_diagnostico = R.olho_esquerdo_perto
WHERE
	C.id_consulta = @id</CommandText>
