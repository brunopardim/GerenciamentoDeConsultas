<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoIntegrado.View"
        xmlns:materialDesignMahApps="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="ProjetoIntegrado.View.WindowTeste"
        mc:Ignorable="d"
        Title="WindowTeste" 
        Height="510.5" 
        Width="726.417"
>

    <Grid Margin="-0">
        <TextBox x:Name="tbEsferico" TabIndex="6" HorizontalAlignment="Left" TextAlignment="Center" Height="38" Width="99" TextWrapping="Wrap"      CharacterCasing="Upper" materialDesignMahApps:HintAssist.Hint="ESFÉRICO" Style="{StaticResource MaterialDesignFloatingHintTextBox}"    VerticalAlignment="Top" Margin="160,22,0,0"  />
        <TextBox x:Name="tbCilindro" TabIndex="6" HorizontalAlignment="Left" TextAlignment="Center" Height="38" Width="99" TextWrapping="Wrap"      CharacterCasing="Upper" materialDesignMahApps:HintAssist.Hint="CILINDRO" Style="{StaticResource MaterialDesignFloatingHintTextBox}"    VerticalAlignment="Top" Margin="302,22,0,0"  />
        <TextBox x:Name="tbAdicao" TabIndex="6" HorizontalAlignment="Left" TextAlignment="Center" Height="38" Width="99" TextWrapping="Wrap"      CharacterCasing="Upper" materialDesignMahApps:HintAssist.Hint="ADIÇÃO" Style="{StaticResource MaterialDesignFloatingHintTextBox}"    VerticalAlignment="Top" Margin="441,22,0,0"  />
        <TextBox x:Name="tbEixo" TabIndex="6" HorizontalAlignment="Left" TextAlignment="Center" Height="38" Width="99" TextWrapping="Wrap"      CharacterCasing="Upper" materialDesignMahApps:HintAssist.Hint="EIXO" Style="{StaticResource MaterialDesignFloatingHintTextBox}"    VerticalAlignment="Top" Margin="582,22,0,0"  />
    </Grid>
</Window>
